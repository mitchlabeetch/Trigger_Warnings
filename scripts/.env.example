# Does the Dog Die API Configuration
# Get your API key from: https://www.doesthedogdie.com/profile

DTDD_API_KEY=your_api_key_here

# Quality Thresholds
CONFIDENCE_THRESHOLD=0.7  # 0.0 to 1.0
MIN_VOTES=5               # Minimum total votes required

# Rate Limiting
REQUEST_DELAY_MS=1000     # Delay between API requests

# Output Settings
OUTPUT_FORMAT=sql         # sql or json
OUTPUT_DIR=../database    # Where to save output files
